{
    "name": "Download Bing Create",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "Download all generated images from Bing Image Creator",
	"background" : {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [
		"activeTab",
		"downloads",
		"storage"
	],
	"icons": {
		"128": "icon.png"
	},
	"options_page": "options.html"
}